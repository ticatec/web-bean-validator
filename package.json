{
  "name": "@ticatec/web-bean-validator",
  "version": "0.0.10",
  "type": "module",
  "main": "index.js",
  "description": "This is a tool for automatically validating entities by defining rules, providing boundary checking for strings, numbers, dates, enums, objects, and arrays in JavaScript/TypeScript applications.",
  "types": "index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsc && cp ./package.json ./dist/ && cp ./README.md ./dist/",
    "publish:public": "cd ./dist && npm publish --access public"
  },
  "keywords": [
    "validation",
    "entity-validator",
    "boundary-checking",
    "javascript",
    "typescript",
    "rules-based",
    "data-validation"
  ],
  "devDependencies": {
    "@ticatec/i18n": "^0.0.5",
    "dayjs": "^1.11.13"
  },
  "repository": {
    "type": "https",
    "url": "https://github.com/ticatec/web-bean-validator.git"
  },
  "bugs": {
    "url": "https://github.com/ticatec/web-bean-validator/issues"
  },
  "homepage": "https://github.com/ticatec/web-bean-validator",
